{
    "name": "staxr/querybuilder",
    "type": "library",
    "description": "General QueryBuilder",
    "keywords": [
        "annotations",
        "docblock",
        "tools"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Noramarth <Dan Radu Dragomir>",
            "email": "noramarth@aom.ro"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "codeception/codeception": "^4.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "StaXr\\QueryBuilder\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StaXr\\Tests\\QueryBuilder\\": "tests/StaXr/QueryBuilder"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.2-dev"
        }
    }
}